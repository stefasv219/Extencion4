{
    "manifest_version" :2,
    "name":"calendar",
    "description": "Muestra el calendario de google",
    "version":"1.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    },
    "permissions": [ "https://apis.google.com/"]
}